{
  "version": 2,
  "builds": [
    {
      "src": "src/api/main.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "src/api/main.py"
    }
  ],
  "env": {
    "ALIEXPRESS_APP_KEY": "@aliexpress-app-key",
    "ALIEXPRESS_APP_SECRET": "@aliexpress-app-secret",
    "ALIEXPRESS_TRACKING_ID": "gpt_chat",
    "ALIEXPRESS_LANGUAGE": "EN",
    "ALIEXPRESS_CURRENCY": "USD",
    "LOG_LEVEL": "INFO"
  },
  "functions": {
    "src/api/main.py": {
      "maxDuration": 30
    }
  }
}