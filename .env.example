# AliExpress API Configuration (Required)
ALIEXPRESS_APP_KEY=your_aliexpress_app_key_here
ALIEXPRESS_APP_SECRET=your_aliexpress_app_secret_here

# Optional API Token for Proxy Authentication
# If set, clients must provide this token to access the proxy
API_TOKEN=your_optional_api_token_here

# Rate Limiting Configuration (Optional)
RATE_LIMIT_MAX=100
RATE_LIMIT_WINDOW=60000

# Node Environment
NODE_ENV=production